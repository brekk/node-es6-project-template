{
  "name": "es6-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run build",
    "test-es6": "./node_modules/.bin/babel-tape-runner \"tests/**/spec-*.js\"",
    "test-core": "npm run transpile && npm run test-es6",
    "test-faucet": "npm run test-core | ./node_modules/.bin/faucet",
    "test-log": "npm run test-core > test-output.log",
    "cover": "./node_modules/.bin/nyc -x=lib/**/*.js -x=tests/**/*.js --reporter=lcov --reporter=json --reporter=text --reporter=text-summary npm run test-faucet",
    "report": "./node_modules/.bin/istanbul report",
    "transpile": "babel src -d lib",
    "inception": "npm run build && ./cli.js coverage/coverage-final.json",
    "build": "npm run cover && npm run report",
    "prepublish": "npm run build"
  },
  "bin": {
    "cya": "./cli.js"
  },
  "keywords": [
  ],
  "author": "brekk",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-lodash-fp": "^2.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "faucet": "0.0.1",
    "must": "^0.13.2",
    "nyc": "^7.1.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "data.validation": "^1.3.1",
    "fixture-stdout": "^0.2.1",
    "git-js": "^1.1.1",
    "istanbul": "^0.4.4",
    "lodash": "^4.14.1",
    "parse-git-status": "^0.1.0"
  }
}
